{
  "name": "cc-map-editor",
  "version": "0.12.0",
  "license": "MIT",
  "repository": "https://github.com/CCDirectLink/crosscode-map-editor",
  "main": "main.js",
  "build": {
    "appId": "CCDirectLink.crosscode.map.editor",
    "artifactName": "${productName}-${version}-${os}.${ext}",
    "files": [
      "main.js",
      "distAngular"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": [
        "nsis"
      ],
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "publish": [
        "github"
      ]
    }
  },
  "browserslist": [
    "Electron >= 4"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:aot": "ng serve --aot",
    "startNoLiveReload": "ng serve --live-reload false",
    "build": "ng build",
    "build:prod": "ng build --prod --no-progress",
    "test": "ng test",
    "lint": "ng lint",
    "electron:dev": "electron . --dev",
    "electron:prod": "electron .",
    "builder:pack": "electron-builder --dir",
    "builder:dist": "electron-builder --publish never",
    "postinstall": "electron-builder install-app-deps",
    "package": "npm run postinstall && npm run build:prod && npm run builder:dist",
    "dist": "npm run build:prod && npm run builder:dist",
    "release": "npm run build:prod && electron-builder --publish always"
  },
  "private": true,
  "dependencies": {
    "electron-context-menu": "^2.3.0",
    "electron-log": "^4.3.0",
    "electron-updater": "^4.3.5",
    "electron-window-state": "^5.0.3",
    "node-ipc": "^9.1.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1200.0",
    "@angular-devkit/build-angular": "^12.0.0",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/animations": "^12.0.0",
    "@angular/cdk": "^12.0.0",
    "@angular/cli": "^12.0.0",
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.0.0",
    "@angular/language-service": "^12.0.0",
    "@angular/material": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "@babylonjs/core": "^4.2.0",
    "@babylonjs/gui": "^4.2.0",
    "@babylonjs/inspector": "^4.2.0",
    "@babylonjs/loaders": "^4.2.0",
    "@babylonjs/materials": "^4.2.0",
    "@babylonjs/post-processes": "^4.2.0",
    "@babylonjs/procedural-textures": "^4.2.0",
    "@babylonjs/serializers": "^4.2.0",
    "@types/earcut": "^2.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/jsoneditor": "^8.6.0",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "ajv-formats": "^2.1.0",
    "angular2-draggable": "^2.3.2",
    "cc-map-editor-common": "../common",
    "core-js": "^3.8.0",
    "earcut": "^2.2.2",
    "electron": "^10.1.6",
    "electron-builder": "^22.9.1",
    "eslint": "^7.14.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "jsoneditor": "^9.1.4",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "phaser": "3.24.1",
    "rxjs": "^6.6.3",
    "typescript": "~4.2.4",
    "zone.js": "~0.11.4"
  }
}
